{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM", "WebWorker"],
    "skipLibCheck": true,
    "allowJs": true,
    "noImplicitAny": false,
    "outDir": "dist",
    "declaration": true,
    "types": ["vite/client", "@vuepress/client/types", "../typings/components"],
    "paths": {
      "vuesax-alpha": ["../packages/vuesax-alpha"],
      "~/*": ["./.vuepress/theme/*"],
      "@vuesax-alpha/docs": ["./.vuepress"]
    }
  },
  "include": ["**/*", ".vuepress/**/*"],
  "exclude": ["node_modules"]
}
